<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services
        https://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <defaults autowire="true" autoconfigure="true"/>

        <service id="skillbox.article_content_provider" class="SkillboxSymfony\ArticleContentProviderBundle\ArticleContentProvider" public="true"/>
        <service id="SkillboxSymfony\ArticleContentProviderBundle\ArticleContentProvider" alias="skillbox.article_content_provider" public="false"/>

        <service id="skillbox.comment_content_provider" class="SkillboxSymfony\ArticleContentProviderBundle\CommentContentProvider" public="true"/>
        <service id="SkillboxSymfony\ArticleContentProviderBundle\CommentContentProvider" alias="skillbox.comment_content_provider" public="false"/>

        <service id="skillbox.paste_words_service" class="SkillboxSymfony\ArticleContentProviderBundle\PasteWordsService" public="true"/>
        <service id="SkillboxSymfony\ArticleContentProviderBundle\PasteWordsService" alias="skillbox.paste_words_service" public="false"/>
    </services>
</container>